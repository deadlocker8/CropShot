<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="600.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.SelectController">
   <children>
      <VBox fx:id="vbox" prefHeight="600.0" prefWidth="800.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <AnchorPane VBox.vgrow="ALWAYS">
               <children>
                  <StackPane fx:id="stackPane" layoutX="25.0" layoutY="25.0" AnchorPane.bottomAnchor="25.0" AnchorPane.leftAnchor="25.0" AnchorPane.rightAnchor="25.0" AnchorPane.topAnchor="25.0">
                     <children>
                        <Pane fx:id="paneImage" prefHeight="200.0" prefWidth="200.0" StackPane.alignment="CENTER" />
                        <Pane fx:id="pane" prefHeight="200.0" prefWidth="200.0" StackPane.alignment="CENTER" />
                     </children></StackPane>
               </children>
            </AnchorPane>
            <Separator prefWidth="800.0" VBox.vgrow="NEVER" />
            <AnchorPane prefHeight="210.0" prefWidth="800.0" VBox.vgrow="NEVER">
               <children>
                  <VBox alignment="CENTER" layoutX="25.0" layoutY="34.0" prefHeight="90.0" prefWidth="350.0" AnchorPane.bottomAnchor="20.0" AnchorPane.leftAnchor="75.0" AnchorPane.rightAnchor="75.0">
                     <children>
                        <HBox alignment="CENTER" prefHeight="35.0" prefWidth="350.0" VBox.vgrow="ALWAYS">
                           <children>
                              <Label alignment="CENTER_RIGHT" prefWidth="50.0" text="Breite:">
                                 <font>
                                    <Font name="System Bold" size="15.0" />
                                 </font>
                              </Label>
                              <Slider fx:id="sliderWidth" minorTickCount="1" prefHeight="15.0" prefWidth="400.0" snapToTicks="true" HBox.hgrow="ALWAYS">
                                 <HBox.margin>
                                    <Insets left="25.0" />
                                 </HBox.margin>
                              </Slider>
                              <TextField fx:id="textFieldWidth" prefHeight="25.0" prefWidth="60.0">
                                 <HBox.margin>
                                    <Insets left="15.0" />
                                 </HBox.margin>
                              </TextField>
                           </children>
                        </HBox>
                        <HBox alignment="CENTER" prefHeight="35.0" prefWidth="325.0" VBox.vgrow="ALWAYS">
                           <children>
                              <Label alignment="CENTER_RIGHT" prefWidth="50.0" text="Höhe:">
                                 <font>
                                    <Font name="System Bold" size="15.0" />
                                 </font>
                              </Label>
                              <Slider fx:id="sliderHeight" minorTickCount="1" prefHeight="15.0" prefWidth="400.0" snapToTicks="true" HBox.hgrow="ALWAYS">
                                 <HBox.margin>
                                    <Insets left="25.0" />
                                 </HBox.margin>
                              </Slider>
                              <TextField fx:id="textFieldHeight" prefHeight="25.0" prefWidth="60.0">
                                 <HBox.margin>
                                    <Insets left="15.0" />
                                 </HBox.margin>
                              </TextField>
                           </children>
                           <VBox.margin>
                              <Insets top="10.0" />
                           </VBox.margin>
                        </HBox>
                        <HBox alignment="CENTER" prefHeight="35.0" prefWidth="350.0" VBox.vgrow="ALWAYS">
                           <children>
                              <Label alignment="CENTER_RIGHT" prefWidth="50.0" text="X:">
                                 <font>
                                    <Font name="System Bold" size="15.0" />
                                 </font>
                              </Label>
                              <Slider fx:id="sliderX" minorTickCount="1" prefHeight="15.0" prefWidth="400.0" snapToTicks="true" HBox.hgrow="ALWAYS">
                                 <HBox.margin>
                                    <Insets left="25.0" />
                                 </HBox.margin>
                              </Slider>
                              <TextField fx:id="textFieldX" prefHeight="25.0" prefWidth="60.0">
                                 <HBox.margin>
                                    <Insets left="15.0" />
                                 </HBox.margin>
                              </TextField>
                           </children>
                           <VBox.margin>
                              <Insets top="10.0" />
                           </VBox.margin>
                        </HBox>
                        <HBox alignment="CENTER" prefHeight="35.0" prefWidth="325.0" VBox.vgrow="ALWAYS">
                           <children>
                              <Label alignment="CENTER_RIGHT" contentDisplay="RIGHT" prefWidth="50.0" text="Y:">
                                 <font>
                                    <Font name="System Bold" size="15.0" />
                                 </font>
                              </Label>
                              <Slider fx:id="sliderY" minorTickCount="1" prefHeight="15.0" prefWidth="400.0" snapToTicks="true" HBox.hgrow="ALWAYS">
                                 <HBox.margin>
                                    <Insets left="25.0" />
                                 </HBox.margin>
                              </Slider>
                              <TextField fx:id="textFieldY" prefHeight="25.0" prefWidth="60.0">
                                 <HBox.margin>
                                    <Insets left="15.0" />
                                 </HBox.margin>
                              </TextField>
                           </children>
                           <VBox.margin>
                              <Insets top="10.0" />
                           </VBox.margin>
                        </HBox>
                        <HBox alignment="CENTER" prefHeight="100.0" prefWidth="100.0" VBox.vgrow="NEVER">
                           <children>
                              <Button fx:id="buttonOK" mnemonicParsing="false" onAction="#buttonOK" text="Übernehmen">
                                 <font>
                                    <Font name="System Bold" size="13.0" />
                                 </font>
                              </Button>
                              <Button fx:id="buttonCancel" mnemonicParsing="false" onAction="#buttonCancel" text="Abbrechen">
                                 <font>
                                    <Font name="System Bold" size="13.0" />
                                 </font>
                                 <HBox.margin>
                                    <Insets left="50.0" />
                                 </HBox.margin>
                              </Button>
                           </children>
                           <VBox.margin>
                              <Insets top="15.0" />
                           </VBox.margin>
                        </HBox>
                     </children>
                  </VBox>
               </children>
            </AnchorPane>
         </children>
      </VBox>
   </children>
</AnchorPane>
